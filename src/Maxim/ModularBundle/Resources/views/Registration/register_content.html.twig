{% trans_default_domain 'FOSUserBundle' %}

{% if not is_granted('IS_AUTHENTICATED_FULLY') %}
    <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
        {{ form_widget(form) }}
        <div>
            <button type="submit" value="{{ 'registration.submit'|trans }}" class="btn btn-default">{{ 'registration.submit'|trans }}</button>

        </div>
    </form>
{% else %}
    <p>You are logged in, you are being redirected to the home page in <span id="redirect_countdown">5</span></p>
{% endif %}
